{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7179
        }
      ],
      "UpstreamPathTemplate": "/profiles/api/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "SwaggerKey": "profiles"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7246
        }
      ],
      "UpstreamPathTemplate": "/services/api/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "SwaggerKey": "services"
    },
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7179
        }
      ],
      "UpstreamPathTemplate": "/profiles/swagger/v1/swagger.json",
      "SwaggerKey": "profiles"
    },
    {
      "DownstreamPathTemplate": "/swagger/v1/swagger.json",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7246
        }
      ],
      "UpstreamPathTemplate": "/services/swagger/v1/swagger.json",
      "SwaggerKey": "services"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "profiles",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Profiles API",
          "Version": "v1",
          "Url": "https://localhost:7179/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "services",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Services API",
          "Version": "v1",
          "Url": "https://localhost:7246/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:8000"
  }
}